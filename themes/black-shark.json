			"settings": {
				"foreground": "#52FFAB"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#FF52A6"
			}
		},
		{
			"name": "Parameter",
			"scope": [
				"variable.parameter",
			],
			"settings": {
				"foreground": "#FFA989"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
